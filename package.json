{
  "name": "whiteboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "konva": "^10.0.2",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.1",
    "react-konva": "^19.0.10",
    "react-konva-utils": "^2.0.0",
    "react-router-dom": "^7.9.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "uuid": "^13.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t owlboard-frontend .",
    "docker:build:dev": "docker build --target development -t owlboard-frontend:dev .",
    "docker:build:prod": "docker build --target production -t owlboard-frontend:prod .",
    "docker:run:dev": "docker run -p 3000:3000 --name owlboard-frontend-dev owlboard-frontend:dev",
    "docker:run:prod": "docker run -p 3000:80 --name owlboard-frontend-prod owlboard-frontend:prod",
    "docker:stop": "docker stop owlboard-frontend-dev owlboard-frontend-prod || true",
    "docker:clean": "docker rm owlboard-frontend-dev owlboard-frontend-prod || true"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/react": "^19.1.13",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "cross-env": "^10.0.0"
  }
}
